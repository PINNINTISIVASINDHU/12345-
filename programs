#include <stdio.h>

int main() {
float c,f;
printf("enter temp in celsius:");
scanf("%f",&c);
f =( ((9*c)/5)+ 32);
printf("temp in fahernheit = %f",f);
    return 0;
}
//q=7
#include<stdio.h>
int main(){
	int X=23,Y=-11,Z=2,D=-56,A=8;
	float r;
	r = (X+Y-2*Z%D/2-A++);
	printf("%f",r);
}
//q=8
#include <stdio.h>

int main() {
int a[60],n,sum=0;
float r;
printf("enter no of users:");
scanf("%d",&n);
for(int i =0;i<n;i++){
scanf("%d",&a[i]);
sum =sum+a[i];}
r=(sum/n);
printf("average =%f",r);
    return 0;
}
